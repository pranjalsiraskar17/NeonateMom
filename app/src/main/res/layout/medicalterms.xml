<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             android:background="@drawable/bc"


>
    <TextView
            android:background="@drawable/ll"
            android:layout_width="match_parent"
            android:text="Medical Terms"
            android:textSize="30dp"
            android:textColor="#FFFFFF"
            android:gravity="center"
            android:layout_height="45dp"
            android:id="@+id/textView13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:background="@drawable/back"
            android:layout_width="43dp"
            android:layout_height="37dp"
            android:id="@+id/textView16" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="4dp" app:layout_constraintTop_toTopOf="parent"/>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/ScrollView01"
                android:paddingTop="45dp"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" tools:layout_editor_absoluteY="27dp"
                tools:layout_editor_absoluteX="0dp">
        <RelativeLayout
                android:id="@+id/RelativeLayout01"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

            <LinearLayout
                    android:id="@+id/LinearLayout01"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="0dp">
                <Button
                        android:text="Adjusted Age"
                        android:layout_width="127dp"

                        android:textSize="18sp"
                        android:textAllCaps="false"
                        android:background="@null"
                        android:layout_height="39dp"
                        android:id="@+id/button4"
                        tools:text="Adjusted Age" app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginLeft="16dp"
                        android:layout_marginStart="16dp" android:layout_marginTop="28dp"
                        android:textStyle="italic"
                        app:layout_constraintTop_toTopOf="parent"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="   Auditory Brainstem Response Test"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        android:layout_width="306dp"
                        android:background="@null"
                        android:layout_height="44dp"
                        android:id="@+id/button5"
                        tools:layout_editor_absoluteX="0dp" android:layout_marginTop="10dp"
                        app:layout_constraintTop_toBottomOf="@+id/button4"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="   Bradycardia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="212dp"
                        android:id="@+id/button6" android:layout_marginStart="8dp"
                        app:layout_constraintStart_toStartOf="parent"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        android:layout_marginLeft="8dp"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Broncho-pulmonary Dysplasia(BPD)"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="257dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="    Cannula"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="309dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button2"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Catheter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="358dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="0dp" android:id="@+id/button3"/>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Developmental Care"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="398dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button7"/>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Developmental Delays"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="446dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button8"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Echo-cardiogram"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="498dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button9"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Electrodes"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:layout_editor_absoluteX="65dp" android:id="@+id/button11"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        app:layout_constraintTop_toBottomOf="parent"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="     Gastro-enterologist"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="511dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button13"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>

            </LinearLayout>
        </RelativeLayout>

    </ScrollView>


    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                          android:orientation="vertical"/>
    <TextView
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@drawable/home"
            android:id="@+id/back_to_register_textview"
            app:layout_constraintBottom_toBottomOf="parent"/>


</android.support.constraint.ConstraintLayout>