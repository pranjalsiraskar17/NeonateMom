<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:background="@drawable/bc"
                                             android:layout_height="match_parent"


>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/ScrollView01"
                android:layout_width="fill_parent"
                android:paddingTop="45dp"
                android:layout_height="fill_parent" tools:layout_editor_absoluteY="27dp"
                tools:layout_editor_absoluteX="0dp">
        <RelativeLayout
                android:id="@+id/RelativeLayout01"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

            <LinearLayout
                    android:id="@+id/LinearLayout01"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="vertical" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="0dp">
                <Button
                        android:text="What is adjusted age vs actual age?"
                        android:layout_width="wrap_content"
                        android:layout_gravity="left"
                        android:textSize="18sp"
                        android:textAllCaps="false"
                        android:background="@null"
                        android:layout_height="39dp"
                        android:id="@+id/button4"
                         app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginLeft="16dp"
                        android:layout_marginStart="16dp" android:layout_marginTop="28dp"
                        android:textStyle="italic"
                        app:layout_constraintTop_toTopOf="parent"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="Can't just baby go home when he hits a certain weight?"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:layout_gravity="left"
                        android:textSize="18sp"
                        android:layout_width="333dp"
                        android:background="@null"
                        android:layout_height="42dp"
                        android:id="@+id/button5"
                        tools:layout_editor_absoluteX="0dp" android:layout_marginTop="10dp"
                        app:layout_constraintTop_toBottomOf="@+id/button4"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="  Will there be long-term developmental issues?"
                        android:layout_width="wrap_content"
                        android:layout_height="47dp" tools:layout_editor_absoluteY="212dp"
                        android:id="@+id/button6" android:layout_marginStart="8dp"
                        app:layout_constraintStart_toStartOf="parent"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:layout_gravity="left"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        android:layout_marginLeft="8dp"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="Will he be like a 'typical' baby when he comes home?"
                        android:layout_width="374dp"
                        android:layout_height="47dp" tools:layout_editor_absoluteY="257dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:layout_gravity="left"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="    What happened?"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="309dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:layout_gravity="left"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button2"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="    What are all the chords for?"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="358dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:layout_gravity="left"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="0dp" android:id="@+id/button3"/>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="  What is kangaroo care and what are its benefits?"
                        android:layout_width="327dp"
                        android:layout_height="45dp" tools:layout_editor_absoluteY="398dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:layout_gravity="left"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button7"/>

                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text=" How can I maintain breast milk as the main source of nutrition for my baby?"
                        android:layout_width="364dp"
                        android:layout_height="49dp" tools:layout_editor_absoluteY="446dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        android:layout_gravity="left"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button8"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="   What are the different levels of neonatal care?"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="498dp"
                        android:background="@null"
                        android:layout_gravity="left"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button9"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="   How long will my baby be in the NICU?"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:layout_editor_absoluteX="65dp" android:id="@+id/button11"
                        android:background="@null"
                        android:layout_gravity="left"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        app:layout_constraintTop_toBottomOf="parent"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>
                <Button
                        android:text="   How much does a preemie weigh?"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" tools:layout_editor_absoluteY="511dp"
                        android:background="@null"
                        android:textAllCaps="false"
                        android:textStyle="italic"
                        android:textSize="18sp"
                        android:layout_gravity="left"
                        tools:layout_editor_absoluteX="16dp" android:id="@+id/button13"/>
                <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@android:color/darker_gray"/>

            </LinearLayout>
        </RelativeLayout>

    </ScrollView>


    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                          android:orientation="vertical"/>
    <TextView
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@drawable/home"
            android:id="@+id/back_to_register_textview"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:background="@drawable/ll"
            android:layout_width="match_parent"
            android:text="FAQs"
            android:textSize="30dp"
            android:textColor="#FFFFFF"
            android:gravity="center"
            android:layout_height="45dp"
            android:id="@+id/textView13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:background="@drawable/back"
            android:layout_width="43dp"
            android:layout_height="37dp"
            android:id="@+id/textView16" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="4dp" app:layout_constraintTop_toTopOf="parent"/>



</android.support.constraint.ConstraintLayout>