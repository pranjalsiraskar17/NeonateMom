<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent" android:id="@+id/text_nurse"
android:background="@drawable/bc">

    <TextView
            android:background="@drawable/ll"
            android:layout_width="match_parent"
            android:text="Welcome Username"
            android:textSize="30dp"
            android:textColor="#FFFFFF"
            android:gravity="center"
            android:layout_height="45dp"
            android:id="@+id/textView13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:background="@drawable/back"
            android:layout_width="43dp"
            android:layout_height="37dp"
            android:id="@+id/textView16" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="4dp" app:layout_constraintTop_toTopOf="parent"/>

    <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp" app:srcCompat="@drawable/growthtracking"
            android:scaleType="centerCrop"
            android:id="@+id/growth_button" app:layout_constraintStart_toStartOf="parent"
            android:background="@null"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" android:layout_marginTop="70dp"

            app:layout_constraintTop_toTopOf="parent"/>
    <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp" app:srcCompat="@drawable/guide1"
            android:id="@+id/guide_button" app:layout_constraintStart_toEndOf="@+id/mother-button"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="70dp"
            android:background="@null"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"/>
    <TextView
            android:text="User Guide"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/text_physician" tools:id="text_physician"
            android:textColor="#004481"
            android:textStyle="bold"
            android:textSize="26sp"
            app:layout_constraintTop_toBottomOf="@+id/guide_button"
            android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="@+id/guide_button"
            app:layout_constraintEnd_toEndOf="@+id/guide_button"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp"/>
    <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp" app:srcCompat="@drawable/experience1"
            android:id="@+id/experience_button" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mother-button"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@null"
            app:layout_constraintVertical_bias="0.43" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_marginBottom="148dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" app:layout_constraintHorizontal_bias="0.0"/>
    <ImageButton
            android:layout_width="120dp"
            android:layout_height="120dp" app:srcCompat="@drawable/settings1"
            android:id="@+id/experience_button1" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/mother-button"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@null"
            app:layout_constraintVertical_bias="0.43" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_marginBottom="148dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="Experience Sharing"
            android:layout_width="140dp"
            android:layout_height="70dp"
            android:gravity="center"
            android:id="@+id/textView6"
            android:textColor="#004481"
            android:textStyle="bold"
            android:textSize="26sp"
            app:layout_constraintEnd_toEndOf="@+id/experience_button"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="@+id/experience_button"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.535"
            app:layout_constraintTop_toBottomOf="@+id/experience_button" android:layout_marginTop="12dp"/>
    <TextView
            android:text="Setting"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:id="@+id/textView6"
            android:textColor="#004481"
            android:textStyle="bold"
            android:textSize="26sp"
            app:layout_constraintStart_toStartOf="@+id/experience_button1"
            app:layout_constraintEnd_toEndOf="@+id/experience_button1" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/experience_button1" app:layout_constraintHorizontal_bias="0.542"/>
    <TextView
            android:text="Logout"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textColor="@android:color/white"
            android:background="@drawable/ll"
            android:textStyle="bold"
            android:textSize="20dp"
            android:id="@+id/back_to_register_textview"
            tools:background="#004481"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Growth Tracking"
            android:layout_width="120dp"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="26sp"
            android:textColor="#004481"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="196dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.087"/>

</android.support.constraint.ConstraintLayout>